    "compilerOptions": {
        "noEmit": true,
        "target": "esnext",
        "module": "esnext",
        "allowJs": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    }
}